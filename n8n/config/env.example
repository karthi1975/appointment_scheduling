# n8n Environment Configuration
# Copy this file to .env and update with your actual values

# n8n Basic Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=localdev123

# n8n Host Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# Database Configuration (PostgreSQL)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=database
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=medical_booking
DB_POSTGRESDB_USER=admin
DB_POSTGRESDB_DATABASE_PASSWORD=localdev123

# n8n Execution Settings
N8N_EXECUTIONS_PROCESS=main
N8N_EXECUTIONS_MODE=regular
N8N_EXECUTIONS_TIMEOUT=3600

# Webhook Configuration
N8N_WEBHOOK_URL=http://localhost:5678/
N8N_WEBHOOK_TEST_URL=http://localhost:5678/

# Security Settings
N8N_SECURITY_OAUTH2_DISABLED=false
N8N_SECURITY_OAUTH2_ISSUER_URL=
N8N_SECURITY_OAUTH2_CLIENT_ID=
N8N_SECURITY_OAUTH2_CLIENT_SECRET=

# Logging
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console

# External API Keys (Update these with your actual keys)
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_CALENDAR_CLIENT_ID=your_google_client_id_here
GOOGLE_CALENDAR_CLIENT_SECRET=your_google_client_secret_here
VAPI_API_KEY=your_vapi_api_key_here
VAPI_AGENT_ID=your_vapi_agent_id_here

# Insurance API Configuration
INSURANCE_API_KEY=your_insurance_api_key_here
INSURANCE_ENDPOINT=https://api.insurance-provider.com

# Notification Services
TWILIO_ACCOUNT_SID=your_twilio_sid_here
TWILIO_AUTH_TOKEN=your_twilio_token_here
SENDGRID_API_KEY=your_sendgrid_key_here 